<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">
        <activity
            android:name=".CircularsActivity"
            android:label="@string/title_activity_circulars"
            android:theme="@style/Theme.MyApp.NoActionBar"></activity>
        <activity
            android:name=".NoticesActivity"
            android:label="@string/title_activity_notices"
            android:theme="@style/Theme.MyApp.NoActionBar" />
        <activity
            android:name=".CoursesActivity"
            android:label="@string/title_activity_courses"
            android:theme="@style/Theme.MyApp.NoActionBar" />
        <activity
            android:name=".EventsActivity"
            android:label="@string/title_activity_events"
            android:theme="@style/Theme.MyApp.NoActionBar" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomePageActivity" />
    </application>

</manifest>